

!!!!!!!!!!! XTERM !!!!!!!!!!!!!!!!
XTerm*renderFont: true
XTerm*termName:   xterm-256color
XTerm*locale: utf-8
XTerm*metaSendsEscape: true
XTerm*backarrowKey: false
XTerm*ttyModes: erase ^?

!! COLOR !!
!XTerm*Background: #220128
XTerm*Background: black
XTerm*Foreground: white

!! WINDOW !!
!XTerm*BorderWidth: 0

!! FONT !!
XTerm*faceName: DejaVuSansMono Nerd Font:style=Book:size=8
!!XTerm*faceName: TerminessTTF NF:style=Regular:antialias=true:hinting=true:autohint=true:size=10


!!!!!!!!!!! URXVT !!!!!!!!!!!!!!!!

!! COLOR !!
URxvt*background: black
URxvt*foreground: white

!!!!!!!!!!!!!! ROFI !!!!!!!!!!!!!!
!color for certain lines
!outside borders
rofi.color-window: #151523,#646591
!                  bg   fg   bgalt hlbg hlfg
rofi.color-normal: #1a1a33,#646591,#212142,#333366
!rofi.color-active: #333,#333
!rofi.bg: #333
!rofi.fg: #666
